if [[ -f ~/.proxy ]]; then
		source ~/.proxy
fi
set -xv
source ~/.functions.sh
chmod a+x bin/*
export TMPDIR=~/tmp && mkdir ~/tmp || echo "Failed to make temporary directory"
chmod +t $TMPDIR || echo "Failed to set sticky bit on $TMPDIR"
pathadd .
pathadd $HOME/bin
test -d ~/.linuxbrew || bin/makeLinuxBrew.sh
pathadd ~/.linuxbrew/bin
pathadd ~/.linuxbrew/sbin
hash zsh || bin/makeZSH.sh
set +xv