if [[ -f ~/.proxy ]]; then
		source ~/.proxy
fi
source ~/.functions.sh
chmod a+x bin/*
export TMPDIR=~/tmp && test -d $TMPDIR || mkdir $TMPDIR || echo "Failed to make temporary directory"
chmod +t $TMPDIR || echo "Failed to set sticky bit on $TMPDIR"
test -d ~/.linuxbrew || bin/makeLinuxBrew.sh
pathadd ~/.linuxbrew/bin
pathadd ~/.linuxbrew/sbin
hash zsh || bin/makeZSH.sh
# A package manager for shell scripts and functions.
test -d ~/.basher || git clone https://github.com/basherpm/basher.git ~/.basher
hash fzf || (git clone --depth 1 https://github.com/junegunn/fzf.git ~/.fzf; ~/.fzf/install --all)
pathadd ~/.basher
